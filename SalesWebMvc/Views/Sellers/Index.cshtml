@model IEnumerable<SalesWebMvc.Models.Seller>
@{
    ViewData["Title"] = "Sellers";
}

<h2>@ViewData["Title"]</h2>
<table class="table table-striped table-hover" >
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(Model => Model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.Birthdae)
            </th>
            <th>
                @Html.DisplayNameFor(Model => Model.BaseSalary)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Birthdae)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BaseSalary)
            </td>
        </tr>
        }
    </tbody>

</table>

